{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "logo.png",
      "default_title": "HeadHunter Parser OldSchool"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery.js", "contentscript.js" ],
